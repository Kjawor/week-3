while True:
    password= input("enter a password, must be 8 characters minimum, 12 maximum")
    BAD_PASSWORDS = ['password', 'letmein', 'sesame', 'hello', 'justinbieber']
    if password in BAD_PASSWORDS:
        print("dont use these passwords")
    continue
    if len(password) >12 or len(password)<8:
        print("the length of the password is outside the range")
        continue
    else:
        password2 = input("write the password again")
        if password == password2:
            print("access granted")
        else:
            print("passwords do not match")
        continue
